@model List<IdentityRole<Guid>>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">List of all roles</h5>
        <ul class="list-group list-group-flush container col-6">
            @foreach (var role in Model)
            {
                <li class="list-group-item d-flex justify-content-between align-items-center">
                    @role.Name
                    <span class="badge ps-3 ps-md-0"><a class="btn btn-info" asp-action="EditRole" asp-route-Id="@role.Id">Edit</a></span>
                </li>
            }
        </ul>
    </div>
</div>