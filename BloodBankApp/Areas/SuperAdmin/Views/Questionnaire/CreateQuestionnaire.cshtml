@model HealthFormQuestionnaire
<h1>Create Questionnaire</h1>

<div class="d-flex justify-content-center">
    <div class="col-lg-8">
        <div class="card">
            <div class="card-body p-4">
                <h5 class="card-title">Create Questions</h5>
                <form asp-action="CreateQuestionnaire" method="post">
                    <div class="row mb-3">
                        <div class="col-sm-10">
                            <input asp-for="LastUpdated" type="hidden" class="form-control" />
                        </div>
                    </div>
                    <div class="d-flex justify-content-center">
                        <input type="submit" value="Create Questionnaire" class="btn btn-info" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>